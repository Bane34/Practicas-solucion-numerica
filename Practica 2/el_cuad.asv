function val = phi_1(a, b, x)
    h = b - a;
    m = (a + b) / 2;

    val = 2 * (b - x) * (m - x) / (h^2);
end

function val = phi_2(a, b, x)
    h = b - a;
    m = (a + b) / 2;

    val = 4 * (b - x) * (x - a) / (h^2);
end

function val = phi_3(a, b, x)
    h = b - a;
    m = (a + b) / 2;

    val = 2 * (x - a) * (x - m) / (h^2);
end